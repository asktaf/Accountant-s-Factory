{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/script.js",
      "dest": "/api/script.js"
    },
    {
      "src": "/style.css",
      "dest": "/api/css/script.js"
    }
  ]
}
